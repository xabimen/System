syntax on                                   
set background=dark
colorscheme solarized                         
set t_Co=256                        " force vim to use 256 colors
let g:solarized_termcolors=256      " use solarized 256 fallback
hi link fortranTab NONE                     
set number                                  
set tabstop=4                               
set softtabstop=0             
set shiftwidth=4
set expandtab
set autoindent
set smartindent

set laststatus=2


let g:indentLine_setColors = 0
